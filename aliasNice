#Screenbash

deprobe () {
 
    while read name || [ -n "$name" ]; do 
   
           echo "$name" |  awk -F '//' '{print $2}'
    done
}
#FunÃ§oes 

ckdir () {
    mkdir -p $1
    cd $1
}

massnap () {
    
   xargs -t -I {}  sh -c "nmap -A {} | tee {}"

}

beautify () {

    ls | xargs -t -I {} sh -c 'js-beautify {} > beauty.{}; rm {}'
}


